<link rel="stylesheet" href="/css/signUp.css">
<nav>
    <a href="">Sign In</a>
</nav>
<section>
     <h1>Sign Up To Account</h1>
    <form id="form" action="/create_session" method="POST">
          <% if(firstNameError != null){ %>
            <small><%- firstNameError %></small><br>  
          <%}%>
          <input type="text" id="firstname" name="firstname" placeholder="Enter First Name"><br>
          <% if(lastNameError != null){ %>
            <small><%- lastNameError %></small><br>
          <%}%>
          <input type="text" id="lastname" name="lastname" placeholder="Enter Last Name">
          <% if(emailError != null){ %>
            <small><%- emailError %></small><br>
          <%}%>
          <input type="email" id="email" name="email" placeholder="Enter the Email">
          <% if(passwordError != null){ %>
            <small><%- passwordError %></small><br>
          <%}%>
          <input type="password" id="password" name="password" placeholder="Enter the password"><i class="fa-solid fa-circle-check"></i>&nbsp;<i class="fa-solid fa-circle-exclamation"></i><br><br>
          <button id="button" type="submit">Submit</button>
    </form>
</section>


